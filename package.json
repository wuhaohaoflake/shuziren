{
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env PORT=4000 umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev PORT=4000",
    "start:pre": "cross-env REACT_APP_ENV=pre MOCK=none umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@alex_xu/rc-drag": "^1.1.3",
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-layout": "^5.0.12",
    "@types/classnames": "^2.2.10",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.22",
    "antd": "^5.4.0",
    "axios": "^0.21.1",
    "base-64": "^1.0.0",
    "classnames": "^2.2.6",
    "crypto-js": "^4.1.1",
    "excel-loader": "^1.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.14.0",
    "react-cool-dimensions": "^1.2.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.4.3",
    "react-markdown": "^5.0.3",
    "socket.io-client": "^3.0.4",
    "ahooks": "^2.10.9",
    "flooks": "^4.0.2",
    "immer": "^9.0.5",
    "konva": "^8.1.1",
    "lodash": "^4.17.21",
    "mousetrap": "^1.6.5",
    "react-color": "^2.19.3",
    "react-infinite-scroll-component": "^6.1.0",
    "react-konva": "^17.0.2-4",
    "react-konva-utils": "^0.1.7",
    "use-image": "^1.0.7",
    "use-immer": "^0.6.0",
    "umi": "^3.2.22",
    "umi-plugin-keep-alive": "0.0.1-beta.28",
    "umi-request": "^1.3.5",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/base-64": "^1.0.0",
    "@types/crypto-js": "^4.0.2",
    "@types/react-grid-layout": "^1.1.1",
    "cross-env": "^7.0.3"
  }
}
